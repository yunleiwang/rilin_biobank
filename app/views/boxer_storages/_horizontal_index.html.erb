          <li></li>
          <li></li>
        </ul>
      </div>
      <div class="top">
        <div class="layer_box">
          <% (0...@container.container_columns).each do |i| %>  <!--列数-->
              <div class="layer">
                <% (0...@container.container_rows).each do |j| %>
                    <% frame_storage = @frame_storages[(j*@container.container_columns)+i] %>
                    <% if frame_storage.frame_id %>
                        <a class="<%= get_frame_css_class(frame_storage, j, @frame.id) %>" href="/boxer_storages?frame_id=<%= frame_storage.frame_id%>"></a>
                    <% else %>
                        <a class="fancybox_frame<%= get_frame_css_class(frame_storage, j, @frame.id) %>" href="/frames/new?frame_storage_id=<%= frame_storage.id%>" ></a>
                    <% end %>
                <% end %>
              </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
  <div class="r_name_path">
    <i>名称：</i><span>冰箱1</span><i>路径：</i><span>abcdefg</span>
  </div>
</div>
