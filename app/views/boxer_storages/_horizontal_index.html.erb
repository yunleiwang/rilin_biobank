<!-- 冰箱、架子、盒子 -->
<div class="r_upright_shelf_box_big">
  <div class="s_shelf_box_big">
    <div class="shelf">
      <div class="top">
        <div class="layer_box">
          <% (0...@frame.frame_rows).each do |i|  %>
              <div class="layer">

                <% boxer_storage = @boxer_storages[i] %>
                <% if boxer_storage.boxer_id %>
                    <a class="<%= get_css_class2(boxer_storage, 0, nil) %>" href="/sample_storages?boxer_id=<%= boxer_storage.boxer_id%>"></a>
                <% else %>
                    <a class="fancybox_frame<%= get_css_class2(boxer_storage,0, nil) %>" href="/boxers/new?boxer_storage_id=<%= boxer_storage.id%>" ></a>
                <% end %>

              </div>
          <% end %>
        </div>
      </div>
      <div class="bottom">
      </div>
    </div>
  </div>

  <div class="r_name_path">
    <i>名称：</i><span>架子1</span><i>路径：</i><span>abcdefg</span>
  </div>
</div>

<div class="r_sleep_freezer_box_small">
  <div class="s_freezer_box_small">
    <div class="freezer">
      <div class="bottom">
        <div class="hour_icon"></div>
