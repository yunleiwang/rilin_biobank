<%= stylesheet_link_tag "box_big#{@frame.frame_columns}" %>
<%= stylesheet_link_tag "freezer_small#{@container.container_columns}" %>
<div class="row">
  <div class="col-md-12">
    <input class="r_add_btn" type="button" value="添加项目 +">
    <input class="r_modify_btn" type="button" value="修改项目 ✎">
  </div>
</div>
<!-- 冰箱、架子、盒子 -->
<div class="r_upright_shelf_box_big">
  <div class="shelf_box_big">
    <div class="shelf">
      <div class="top">
        <div class="layer_box">
          <% (0...@frame.frame_rows).each do |i| %>
              <div class="layer">
                <% (0...@frame.frame_columns).each do |j| %>
                    <% boxer_storage = @boxer_storages[i*@frame.frame_columns+j] %>
                    <% if boxer_storage.boxer_id %>
                        <a class="fancybox<%= get_css_class2(boxer_storage,j) %>" href="/sample_storages?boxer_id=<%= boxer_storage.boxer_id%>"></a>
                    <% else %>
                        <a class="fancybox_frame<%= get_css_class2(boxer_storage,j) %>" href="/boxers/new?boxer_storage_id=<%= boxer_storage.id%>" ></a>
                    <% end %>
                <% end %>
              </div>
          <% end %>
        </div>
      </div>
      <div class="bottom">
      </div>
    </div>
  </div>
  <div class="r_freezer_rolling_box">
    <ul>
      <li>设备ID</li>
      <li>设备名称</li>
      <li>使用率</li>
