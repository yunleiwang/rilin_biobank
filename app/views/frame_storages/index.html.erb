    <div class="freezer">
      <div class="top">
        <div class="layer_box">
          <% (0...@container.container_rows).each do |i| %>
              <div class="layer">
                <% (0...@container.container_columns).each do |j| %>
                    <% frame_storage = @frame_storages[(i*@container.container_columns)+j] %>

                    <% if frame_storage.frame_id %>
                        <a class="<%= get_frame_css_class(frame_storage,j) %>" href="/boxer_storages?frame_id=<%= frame_storage.frame_id%>"></a>
                    <% else %>
                        <a class="fancybox_frame<%= get_frame_css_class(frame_storage,j) %>" href="/frames/new?frame_storage_id=<%= frame_storage.id%>" ></a>
                    <% end %>

                <% end %>
              </div>
          <% end %>
        </div>
      </div>
      <div class="bottom">
        <div class="hour_icon"></div>
        <ul class="heat_dissipation">
          <li></li>
          <li></li>
          <li></li>
          <li></li>
        </ul>
      </div>
    </div>
