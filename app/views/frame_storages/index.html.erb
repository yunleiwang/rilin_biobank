<%= javascript_include_tag 'jquery.fancybox.js' %>
<%= stylesheet_link_tag 'jquery.fancybox.css' %>
<%= stylesheet_link_tag "freezer_big#{@container.container_columns}" %>
<div class="row">
  <div class="col-md-12">
    <!--添加项目按钮-->
    <input class="r_add_btn" type="button" value="添加项目 +">
    <input class="r_modify_btn" type="button" value="修改项目 ✎">
  </div>
</div>
<!-- 冰箱、架子、盒子 -->

<div class="r_upright_freezer_box_big">
