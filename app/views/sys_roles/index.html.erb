<p id="notice"><%= notice %></p>

<h1>Listing Sys Roles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Remark</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sys_roles.each do |sys_role| %>
      <tr>
        <td><%= sys_role.name %></td>
        <td><%= sys_role.remark %></td>
        <td><%= link_to 'Show', sys_role %></td>
        <td><%= link_to 'Edit', edit_sys_role_path(sys_role) %></td>
        <td><%= link_to 'Destroy', sys_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sys role', new_sys_role_path %>
